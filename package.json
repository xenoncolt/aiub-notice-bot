  "dependencies": {
    "@discordjs/rest": "2.5.1",
    "@types/jsdom": "21.1.7",
    "axios": "^1.11.0",
    "discord-api-types": "^0.38.18",
    "discord.js": "14.21.0",
    "dotenv": "17.2.1",
    "jsdom": "26.1.0",
    "node-fetch": "3.3.2",
    "pdf-parse-new": "1.4.1",
    "pdf-to-png-converter": "3.7.1",
    "sqlite": "5.1.1",
    "sqlite3": "5.1.7",
    "systeminformation": "5.27.7"
