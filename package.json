  "devDependencies": {
    "@types/node": "24.2.0",
    "depcheck": "^1.4.7",
    "npm-check-updates": "^18.0.2",
    "ts-unused-exports": "^11.0.1",
    "tsx": "4.20.3",
    "typescript": "5.9.2"
